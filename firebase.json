{
  "hosting": [{
    "target":"main",
    "public": "build",
    "rewrites": [ {
      "source": "**",
      "destination": "/index.html"
    }], 
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  },
  {
    "target":"testing",
    "public": "build",
    "rewrites": [ {
      "source": "**",
      "destination": "/index.html"
    }], 
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  }
],
  "storage": [{
    "target":"main",
    "rules": "storage.rules"
  }
]
}
